{
  "name": "webbuilder",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "turbo": "^2.5.0",
    "typescript": "5.8.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.8.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@tailwindcss/postcss": "^4.1.4",
    "lucide-react": "^0.546.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.4",
    "zustand": "^5.0.3"
  }
}
